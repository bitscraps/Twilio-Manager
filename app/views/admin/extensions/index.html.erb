<h3>Extensions</h3>

<table class="table table-hover">
	<thead>
		<tr>
			<th>
				Extension
			</th>
			<th>
				Name
			</th>
			<th></th>
			<th></th>
		</tr>
	</thead>
	<% @extensions.each do |ext| %>
		<tr>
			<td>
				<%= ext.username %>
			</td>
			<td>
				<%= ext.name %>
			</td>
			<td><i class="icon-edit"></td>
			<td><i class="icon-trash"></td>
		</tr>
	<% end %>
</table>
