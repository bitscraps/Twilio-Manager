<h3>Administrators</h3>

<table class="table table-hover">
	<thead>
		<tr>
			<th>
				Username
			</th>
			<th>
				Name
			</th>
				<th></th>
			<th></th>
		</tr>
	</thead>
	<% @admins.each do |admin| %>
		<tr>
			<td>
				<%= admin.username %>
			</td>
			<td>
				<%= admin.name %>
			</td>
			<td><i class="icon-edit"></td>
			<td><i class="icon-trash"></td>
		</tr>
	<% end %>
</table>
